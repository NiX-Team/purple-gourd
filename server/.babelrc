{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "~": "./server/src"
        }
      }
    ],
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "env": {
    "development": {},
    "production": {}
  }
}
